<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.yakinorinori.webserver</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>/Users/akinoriyamaguchi/workspace/scripts/mac-mini-startup.sh</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>
    
    <key>StandardOutPath</key>
    <string>/Users/akinoriyamaguchi/workspace/logs/startup.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Users/akinoriyamaguchi/workspace/logs/startup.error.log</string>
    
    <key>WorkingDirectory</key>
    <string>/Users/akinoriyamaguchi/workspace</string>
    
    <key>StartInterval</key>
    <integer>30</integer>
    
    <key>ThrottleInterval</key>
    <integer>10</integer>
</dict>
</plist>
